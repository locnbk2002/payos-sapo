{
    "functions": {
        "public/*.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes": [
        {
            "src": "/checkout.php",
            "dest": "/public/checkout.php"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ]
}